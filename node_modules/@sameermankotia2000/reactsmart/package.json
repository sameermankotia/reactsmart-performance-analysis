{
    "name": "@sameermankotia2000/reactsmart",
    "version": "1.0.0",
    "description": "Machine learning framework for adaptive component loading in React applications",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "lint": "eslint src",
        "format": "prettier --write \"src/**/*.{js,jsx}\"",
        "prepare": "npm run build",
        "prepublishOnly": "npm run test && npm run build",
        "demo:basic": "parcel serve demo/basic/index.html",
        "demo:ecommerce": "parcel serve demo/ecommerce/index.html",
        "demo:content": "parcel serve demo/contentPortal/index.html",
        "demo:dashboard": "parcel serve demo/dashboard/index.html",
        "benchmark": "node experiments/benchmark.js",
        "analyze": "node experiments/dataProcessing/extract.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sameermankotia/reactsmart-performance-analysis.git"
    },
    "keywords": [
        "react",
        "performance",
        "machine-learning",
        "component-loading",
        "adaptive",
        "optimization"
    ],
    "author": "Sameer Mankotia <mank8837@vandals.uidaho.edu>",
    "contributors": [
        "Hasan M Jamil <jamil@uidaho.edu>"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/sameermankotia/reactsmart-performance-analysis/issues"
    },
    "homepage": "https://github.com/sameermankotia/reactsmart-performance-analysis#readme",
    "peerDependencies": {
        "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
        "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0",
        "react-router-dom": "^5.0.0 || ^6.0.0"
    },
    "dependencies": {
        "prop-types": "^15.8.1"
    },
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@rollup/plugin-babel": "^6.0.4",
        "@rollup/plugin-commonjs": "^24.1.0",
        "@rollup/plugin-node-resolve": "^15.3.1",
        "@rollup/plugin-terser": "^0.4.4",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "d3": "^7.8.2",
        "eslint": "^8.33.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "jest": "^29.4.1",
        "jest-environment-jsdom": "^29.4.1",
        "parcel-bundler": "^1.12.5",
        "prettier": "^2.8.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.8.0",
        "rollup": "^3.29.5",
        "rollup-plugin-dts": "^5.1.1",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "typescript": "^4.9.5"
    },
    "jest": {
        "testEnvironment": "jsdom",
        "setupFilesAfterEnv": [
            "@testing-library/jest-dom/extend-expect"
        ],
        "moduleNameMapper": {
            "\\.(css|less|scss)$": "identity-obj-proxy"
        }
    }
}
